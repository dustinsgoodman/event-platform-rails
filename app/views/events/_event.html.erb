<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Timezone:</strong>
    <%= event.timezone %>
  </p>

  <p>
    <strong>Start at:</strong>
    <%= event.start_at %>
  </p>

  <p>
    <strong>End at:</strong>
    <%= event.end_at %>
  </p>

</div>
